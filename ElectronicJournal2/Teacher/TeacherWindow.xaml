<Window x:Class="ElectronicJournal2.Teacher.TeacherWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ElectronicJournal2.Teacher"
        mc:Ignorable="d"
        Title="Электронный журнал" Height="600" Width="800" MinHeight="600" MinWidth="800" Icon="../Resource/TitleLogo.png" WindowStartupLocation="CenterOwner">

    <Grid>

        <Grid.RowDefinitions>

            <RowDefinition Height="53*"/>
            <RowDefinition Height="488*"/>
            <RowDefinition Height="43*"/>

        </Grid.RowDefinitions>

        <Button Name="Btn_Back" Style="{StaticResource BtnBack}" Click="Btn_Back_Click" Visibility="Hidden" Margin="10,12,0,0" VerticalAlignment="Top" Height="40" Width="80"/>

        <Frame Name="Frm_NavPanel" NavigationUIVisibility="Hidden" Margin="0,53,0,0" Grid.RowSpan="3" ContentRendered="Frm_NavPanel_ContentRendered"/>

        <Button  Style="{StaticResource BtnDefault}" Height="30px" Name="Btn_LogOut" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="10 0 0 0" Content="Выход" Click="Btn_LogOut_Click" Grid.Row="2"/>

    </Grid>

</Window>
