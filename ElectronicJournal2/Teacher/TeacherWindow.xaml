<Window x:Class="ElectronicJournal2.Teacher.TeacherWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ElectronicJournal2.Teacher"
        mc:Ignorable="d"
        Title="Электронный журнал" Height="600" Width="800" MinHeight="600" MinWidth="800" Icon="../Resource/TitleLogo.png" WindowStartupLocation="CenterOwner">

    <Grid>

        <Grid.RowDefinitions>

            <RowDefinition Height="1*"/>
            <RowDefinition Height="10*"/>

        </Grid.RowDefinitions>

        <Button Name="Btn_Back" Style="{StaticResource BtnBack}" Click="Btn_Back_Click" Visibility="Hidden"/>

        <Frame Grid.Row="1" Name="Frm_NavPanel" NavigationUIVisibility="Hidden"/>

    </Grid>
    
</Window>
